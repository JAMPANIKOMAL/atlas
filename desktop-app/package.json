{
  "name": "atlas-app",
  "version": "1.0.0",
  "description": "AI Taxonomic Learning & Analysis System",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": "CMLRE",
  "license": "MIT",
  "devDependencies": {
    "electron": "^25.0.0",
    "electron-builder": "^24.6.3"
  },
  "build": {
    "appId": "com.cmlre.atlas",
    "productName": "ATLAS",
    "files": [
      "**/*",
      "!node_modules"
    ],
    "extraResources": [
      {
        "from": "../src",
        "to": "src"
      },
      {
        "from": "../models",
        "to": "models"
      }
    ]
  }
}

